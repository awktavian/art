@prefix agent: <https://awkronos.github.io/web/vocab#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<#museum>
    a agent:AIAgent ;
    dct:title "Patent Museum"@en ;
    dct:description "Digital patent portfolio museum. Capabilities: patent exploration, education, demonstration."@en ;
    agent:capabilities <#caps> .

<#caps>
    a agent:CapabilitySet ;
    dct:title "Museum capabilities"@en ;
    agent:capability <#explore>, <#educate>, <#demonstrate> .

<#explore>
    a agent:Capability ;
    dct:title "Patent exploration"@en ;
    dct:description "Browse 54 patents across 7 wings with proximity-triggered interactions."@en .

<#educate>
    a agent:Capability ;
    dct:title "Education"@en ;
    dct:description "Learn about EFE-CBF, Fano consensus, E8 lattice, world models, crypto, voice, and more."@en .

<#demonstrate>
    a agent:Capability ;
    dct:title "Demonstration"@en ;
    dct:description "Interactive 3D exhibits and link-through to prior art and code."@en .
