/* PRELOADER - Shows while page loads for smooth entrance */
.preloader {
    position: fixed;
    inset: 0;
    background: var(--void);
    z-index: 9999;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    gap: 20px;
    transition: opacity 0.5s, visibility 0.5s;
}
.preloader.hidden { opacity: 0; visibility: hidden; pointer-events: none; }
.preloader-icon {
    font-size: 4rem;
    animation: preloader-bounce 0.8s ease-in-out infinite;
}
@keyframes preloader-bounce {
    0%, 100% { transform: translateY(0) scale(1); }
    50% { transform: translateY(-10px) scale(1.1); }
}
.preloader-bar {
    width: 200px;
    height: 4px;
    background: var(--void-light);
    border-radius: 2px;
    overflow: hidden;
}
.preloader-fill {
    height: 100%;
    background: linear-gradient(90deg, var(--money-green), var(--money-gold));
    width: 0%;
    animation: preloader-fill 1.5s ease-out forwards;
}
@keyframes preloader-fill {
    0% { width: 0%; }
    100% { width: 100%; }
}
/* ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê
   GET A JOB ‚Äî ECONOMIC AUTONOMY FOR ROBOTS
   üíº "Tell the robots to get a job" ‚Äî Kristi, inspiring Kagami's capitalism arc
   
   THE SEQUEL TO "BOSS YOUR CAR AROUND"
   ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê */

:root {
    /* ‚ïê‚ïê‚ïê MONEY PALETTE ‚Äî Premium Wealth Aesthetic ‚ïê‚ïê‚ïê
       Philosophy: Wealth whispers, it doesn't scream.
       The casino neon becomes private bank elegance. */

    --money-green: #32D98C;           /* Warmer emerald ‚Äî confident growth, not traffic light */
    --money-green-dark: #1E9E5A;      /* Deeper foundation */
    --money-green-glow: rgba(50, 217, 140, 0.30);
    --money-green-subtle: rgba(50, 217, 140, 0.06);  /* For breathing room */

    --money-gold: #D4AF37;            /* Old gold ‚Äî the color of actual gold bars */
    --money-gold-bright: #E5C158;     /* Refined highlight */
    --money-gold-subtle: rgba(212, 175, 55, 0.08);

    --profit-orange: #E8A87C;         /* Muted terracotta ‚Äî sophisticated, not alarming */
    --loss-red: #D94F4F;              /* Clear danger without aggression */

    /* ‚ïê‚ïê‚ïê TECH BLUES ‚Äî Sophistication over RGB gamer ‚ïê‚ïê‚ïê */
    --market-blue: #5B8DEF;           /* Warmer, more inviting */
    --market-dark: #2C3E50;
    --stock-cyan: #4ECDC4;            /* Teal ‚Äî tech-forward but mature, harmonizes with green */
    --stock-cyan-subtle: rgba(78, 205, 196, 0.08);

    /* ‚ïê‚ïê‚ïê VOID ‚Äî The canvas that lets colors breathe ‚ïê‚ïê‚ïê */
    --void: #0A0E13;
    --void-light: #111820;
    --void-lighter: #1A232D;
    --void-warm: #12161C;             /* Slightly warmer for variety */

    /* ‚ïê‚ïê‚ïê GLASS ‚Äî Subtle depth without distraction ‚ïê‚ïê‚ïê */
    --glass-bg: rgba(255, 255, 255, 0.02);
    --glass-border: rgba(255, 255, 255, 0.06);
    --glass-glow: rgba(50, 217, 140, 0.08);

    /* ‚ïê‚ïê‚ïê TEXT ‚Äî Clear hierarchy ‚ïê‚ïê‚ïê */
    --text: #f5f5f5;
    --text-dim: rgba(245, 245, 245, 0.55);  /* Slightly more muted for breathing */
    --text-muted: #6B7280;            /* Proper gray for tertiary text */
    --text-bright: #ffffff;
    
    /* Timing (Fibonacci) */
    --dur-fast: 144ms;
    --dur-normal: 233ms;
    --dur-slow: 377ms;
    --dur-dramatic: 610ms;
    --dur-epic: 987ms;
    
    /* Spacing (Fibonacci) */
    --space-xs: 8px;
    --space-sm: 13px;
    --space-md: 21px;
    --space-lg: 34px;
    --space-xl: 55px;
    --space-2xl: 89px;
    
    /* Easing */
    --ease-bounce: cubic-bezier(0.68, -0.55, 0.265, 1.55);
    --ease-smooth: cubic-bezier(0.4, 0, 0.2, 1);
    --ease-money: cubic-bezier(0.25, 0.46, 0.45, 0.94);
}

*, *::before, *::after { box-sizing: border-box; margin: 0; padding: 0; }
html { scroll-behavior: smooth; font-size: 17px; }

body {
    font-family: 'IBM Plex Sans', sans-serif;
    background: var(--void);
    color: var(--text);
    line-height: 1.8;
    overflow-x: hidden;
    -webkit-font-smoothing: antialiased;
}

::selection { background: var(--money-green); color: var(--void); }
::-webkit-scrollbar { width: 8px; }
::-webkit-scrollbar-track { background: var(--void); }
::-webkit-scrollbar-thumb {
    background: var(--void-lighter);  /* Simplified ‚Äî scrollbar shouldn't compete for attention */
    border-radius: 4px;
    transition: background var(--dur-fast);
}
::-webkit-scrollbar-thumb:hover {
    background: var(--money-green);   /* Reward interaction, not distract */
}

/* ‚ïê‚ïê‚ïê MARKET GRID BACKGROUND ‚ïê‚ïê‚ïê */
.market-grid {
    position: fixed;
    inset: 0;
    z-index: -1;
    pointer-events: none;
    background:
        linear-gradient(90deg, rgba(46, 204, 113, 0.02) 1px, transparent 1px),
        linear-gradient(180deg, rgba(46, 204, 113, 0.02) 1px, transparent 1px),
        radial-gradient(ellipse 60% 40% at 20% 30%, rgba(46, 204, 113, 0.04) 0%, transparent 50%),
        radial-gradient(ellipse 40% 35% at 80% 70%, rgba(241, 196, 15, 0.03) 0%, transparent 50%);
    background-size: 50px 50px, 50px 50px, 100% 100%, 100% 100%;
    animation: grid-drift 30s linear infinite;
}

@keyframes grid-drift {
    0% { background-position: 0 0, 0 0, 0 0, 0 0; }
    100% { background-position: 50px 50px, 50px 50px, 0 0, 0 0; }
}

/* ‚ïê‚ïê‚ïê FLOATING MONEY PARTICLES CANVAS ‚ïê‚ïê‚ïê */
#money-particles {
    position: fixed;
    inset: 0;
    z-index: 0;
    pointer-events: none;
    opacity: 0.7;
}

/* ‚ïê‚ïê‚ïê STICKY NAVIGATION ‚ïê‚ïê‚ïê */
.sticky-nav {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 1000;
    background: rgba(10, 14, 19, 0.95);
    backdrop-filter: blur(20px);
    border-bottom: 1px solid var(--glass-border);
    padding: 12px 24px;
    display: flex;
    justify-content: space-between;
    align-items: center;
    transform: translateY(-100%);
    transition: transform var(--dur-normal) var(--ease-smooth);
}

.sticky-nav.visible {
    transform: translateY(0);
}

.nav-logo {
    display: flex;
    align-items: center;
    gap: 10px;
    font-family: 'Bangers', cursive;
    font-size: 1.3rem;
    color: var(--money-green);
    text-decoration: none;
}

.nav-logo span {
    font-size: 1.5rem;
}

.nav-links {
    display: flex;
    gap: 8px;
}

.nav-link {
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.75rem;
    color: var(--text-dim);
    text-decoration: none;
    padding: 8px 14px;
    border-radius: 20px;
    transition: all var(--dur-fast) var(--ease-smooth);
    text-transform: uppercase;
    letter-spacing: 0.05em;
}

.nav-link:hover {
    color: var(--money-green);
    background: rgba(46, 204, 113, 0.1);
}

.nav-cta {
    background: linear-gradient(135deg, var(--money-green), var(--money-green-dark));
    color: var(--void) !important;
    font-weight: 600;
}

.nav-cta:hover {
    transform: scale(1.05);
    box-shadow: 0 4px 15px var(--money-green-glow);
}

@media (max-width: 768px) {
    .nav-links { display: none; }
}

/* ‚ïê‚ïê‚ïê HEADER ‚ïê‚ïê‚ïê */
.header {
    min-height: 100vh;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    text-align: center;
    padding: var(--space-xl) var(--space-md);
    position: relative;
    overflow: hidden;
    z-index: 1;
}

.header::before {
    content: '';
    position: absolute;
    inset: 0;
    background:
        radial-gradient(ellipse 80% 50% at 50% 40%, rgba(46, 204, 113, 0.08) 0%, transparent 60%),
        radial-gradient(ellipse 60% 40% at 30% 60%, rgba(241, 196, 15, 0.05) 0%, transparent 50%);
    animation: header-pulse 8s ease-in-out infinite;
    pointer-events: none;
}

@keyframes header-pulse {
    0%, 100% { opacity: 1; transform: scale(1); }
    50% { opacity: 0.7; transform: scale(1.05); }
}

/* Stock ticker line */
.header::after {
    content: '';
    position: absolute;
    top: 55%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 400px;
    height: 3px;
    background: linear-gradient(90deg,
        transparent 0%,
        rgba(46, 204, 113, 0.1) 20%,
        rgba(46, 204, 113, 0.4) 50%,
        rgba(46, 204, 113, 0.1) 80%,
        transparent 100%
    );
    animation: ticker-pulse 2s ease-in-out infinite;
}

@keyframes ticker-pulse {
    0%, 100% { opacity: 0.3; width: 300px; }
    50% { opacity: 0.6; width: 450px; }
}

/* ‚ïê‚ïê‚ïê BRIEFCASE CONTAINER ‚ïê‚ïê‚ïê */
.header-icon-container {
    position: relative;
    margin-bottom: var(--space-lg);
}

.header-icon {
    font-size: 5rem;
    display: block;
    animation: briefcase-idle 2s var(--ease-money) infinite;
    filter: drop-shadow(0 0 30px var(--money-green-glow));
    position: relative;
    z-index: 2;
}

/* Money floating up */
.header-icon-container::before,
.header-icon-container::after {
    position: absolute;
    font-size: 1.5rem;
    opacity: 0;
    pointer-events: none;
    z-index: 1;
}

.header-icon-container::before {
    content: 'üíµ';
    top: -20px;
    left: 25%;
    animation: money-rise-1 2.5s ease-out infinite;
}

.header-icon-container::after {
    content: 'üí∞';
    top: -15px;
    left: 65%;
    animation: money-rise-2 2.5s ease-out infinite 0.8s;
}

@keyframes briefcase-idle {
    0%, 100% { transform: translateY(0) rotate(-2deg); }
    50% { transform: translateY(-4px) rotate(2deg); }
}

@keyframes money-rise-1 {
    0% { opacity: 0.8; transform: translateY(0) scale(0.5) rotate(0deg); }
    100% { opacity: 0; transform: translateY(-60px) scale(1.2) rotate(20deg); }
}

@keyframes money-rise-2 {
    0% { opacity: 0.6; transform: translateY(0) scale(0.4) rotate(0deg); }
    100% { opacity: 0; transform: translateY(-70px) scale(1) rotate(-15deg); }
}

/* ‚ïê‚ïê‚ïê TITLE ‚ïê‚ïê‚ïê */
.header-title {
    font-family: 'Bangers', cursive;
    font-size: clamp(2.5rem, 10vw, 5.5rem);
    color: var(--money-green);
    text-shadow:
        0 0 10px var(--money-green-glow),
        0 0 30px rgba(46, 204, 113, 0.4),
        4px 4px 0 var(--money-gold),
        8px 8px 0 rgba(241, 196, 15, 0.5),
        12px 12px 20px rgba(0, 0, 0, 0.4);
    letter-spacing: 0.06em;
    line-height: 1.1;
    margin-bottom: var(--space-md);
    animation: title-entrance 1s var(--ease-bounce) both, title-glow 3s ease-in-out infinite 1s;
}

@keyframes title-entrance {
    0% { opacity: 0; transform: scale(0.5) translateY(30px); filter: blur(10px); }
    60% { opacity: 1; transform: scale(1.05) translateY(-5px); filter: blur(0); }
    100% { opacity: 1; transform: scale(1) translateY(0); filter: blur(0); }
}

@keyframes title-glow {
    0%, 100% {
        text-shadow:
            0 0 10px var(--money-green-glow),
            0 0 30px rgba(46, 204, 113, 0.4),
            4px 4px 0 var(--money-gold),
            8px 8px 0 rgba(241, 196, 15, 0.5);
    }
    50% {
        text-shadow:
            0 0 20px rgba(46, 204, 113, 0.8),
            0 0 50px rgba(46, 204, 113, 0.6),
            4px 4px 0 var(--money-gold),
            8px 8px 0 rgba(241, 196, 15, 0.7);
    }
}

/* ‚ïê‚ïê‚ïê SUBTITLE ‚ïê‚ïê‚ïê */
.header-subtitle {
    font-size: 1.25rem;
    color: var(--text);
    max-width: 550px;
    margin-bottom: var(--space-xl);
    opacity: 0;
    animation: subtitle-entrance 0.8s ease-out 0.5s both;
    line-height: 1.6;
}

@keyframes subtitle-entrance {
    0% { opacity: 0; transform: translateY(20px); }
    100% { opacity: 1; transform: translateY(0); }
}

.header-subtitle code {
    color: var(--money-green);
    background: linear-gradient(135deg, rgba(46, 204, 113, 0.15), rgba(241, 196, 15, 0.1));
    padding: 4px 12px;
    border-radius: 6px;
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.95rem;
    border: 1px solid rgba(46, 204, 113, 0.2);
}

/* ‚ïê‚ïê‚ïê SECTIONS ‚ïê‚ïê‚ïê */
section {
    max-width: min(900px, 90vw);
    margin: 0 auto;
    padding: var(--space-2xl) var(--space-md);
    position: relative;
    overflow: hidden;
}

h2 {
    font-family: 'Bangers', cursive;
    font-size: 2.5rem;
    color: var(--money-green);
    text-shadow: 2px 2px 0 var(--money-gold);
    margin-bottom: var(--space-lg);
    display: flex;
    align-items: center;
    gap: var(--space-md);
}

h2 .icon {
    font-size: 2rem;
    opacity: 0;
    transform: translateY(-15px) scale(0.8);
    transition: all var(--dur-dramatic) var(--ease-bounce);
}

h2.visible .icon { opacity: 1; transform: translateY(0) scale(1); }

h3 {
    font-family: 'IBM Plex Sans', sans-serif;
    font-size: 1.4rem;
    color: var(--money-gold);
    margin: var(--space-lg) 0 var(--space-md);
    font-weight: 600;
}

p {
    margin-bottom: var(--space-md);
    color: var(--text-dim);
}

p strong { color: var(--text); font-weight: 600; }

/* ‚ïê‚ïê‚ïê QUOTE BOXES ‚ïê‚ïê‚ïê */
.quote {
    background: linear-gradient(135deg, rgba(46, 204, 113, 0.08), rgba(241, 196, 15, 0.05));
    border-left: 4px solid var(--money-green);
    padding: var(--space-md) var(--space-lg);
    margin: var(--space-lg) 0;
    border-radius: 0 16px 16px 0;
    position: relative;
    transition: all var(--dur-normal) var(--ease-smooth);
}

.quote:hover {
    transform: translateY(-2px);
    box-shadow: 0 8px 30px rgba(46, 204, 113, 0.12);
}

.quote::before {
    content: '"';
    position: absolute;
    top: 10px;
    left: 16px;
    font-family: 'Bangers', cursive;
    font-size: 4rem;
    color: var(--money-green);
    opacity: 0.2;
    line-height: 1;
}

.quote p {
    font-size: 1.2rem;
    font-style: italic;
    color: var(--text);
    margin-bottom: var(--space-sm);
    padding-left: var(--space-md);
}

.quote cite {
    font-style: normal;
    font-size: 0.9rem;
    color: var(--money-gold);
    padding-left: var(--space-md);
}

/* ‚ïê‚ïê‚ïê HERO QUOTE ‚ïê‚ïê‚ïê */
.quote-hero {
    background: linear-gradient(135deg, rgba(46, 204, 113, 0.15), rgba(241, 196, 15, 0.08));
    border-left: none;
    border-radius: 28px;
    padding: var(--space-xl) var(--space-lg) var(--space-2xl);
    margin: var(--space-xl) 0 var(--space-2xl);
    text-align: center;
    position: relative;
    box-shadow: 0 0 80px rgba(46, 204, 113, 0.15), inset 0 1px 0 rgba(255, 255, 255, 0.1);
    animation: hero-glow 4s ease-in-out infinite;
}

@keyframes hero-glow {
    0%, 100% { box-shadow: 0 0 60px rgba(46, 204, 113, 0.12), inset 0 1px 0 rgba(255, 255, 255, 0.1); }
    50% { box-shadow: 0 0 100px rgba(46, 204, 113, 0.25), 0 0 40px rgba(241, 196, 15, 0.1), inset 0 1px 0 rgba(255, 255, 255, 0.15); }
}

.quote-hero::before {
    content: '';
    position: absolute;
    bottom: -16px;
    left: 50%;
    transform: translateX(-50%);
    width: 0;
    height: 0;
    border-left: 20px solid transparent;
    border-right: 20px solid transparent;
    border-top: 20px solid rgba(46, 204, 113, 0.15);
}

.quote-hero::after {
    content: 'üíº';
    position: absolute;
    bottom: -55px;
    left: 50%;
    transform: translateX(-50%);
    font-size: 2.2rem;
    opacity: 0.7;
    animation: briefcase-bounce 2s ease-in-out infinite;
    filter: drop-shadow(0 0 10px rgba(46, 204, 113, 0.3));
}

@keyframes briefcase-bounce {
    0%, 100% { transform: translateX(-50%) rotate(0deg) scale(1); }
    25% { transform: translateX(-50%) rotate(-5deg) scale(1.05); }
    75% { transform: translateX(-50%) rotate(5deg) scale(1.02); }
}

.quote-hero p {
    font-family: 'Bangers', cursive;
    font-size: clamp(1.5rem, 4vw, 2.2rem);
    font-style: normal;
    color: var(--money-green);
    text-shadow: 2px 2px 0 var(--money-gold), 0 0 30px rgba(46, 204, 113, 0.5);
    margin-bottom: var(--space-sm);
    padding-left: 0;
    line-height: 1.3;
}

.quote-hero cite {
    display: block;
    font-family: 'IBM Plex Sans', sans-serif;
    font-size: 1rem;
    font-weight: 400;
    color: var(--text-dim);
    padding-left: 0;
    margin-top: var(--space-xs);
}

/* ‚ïê‚ïê‚ïê CODE BLOCKS ‚ïê‚ïê‚ïê */
.code-block {
    background: var(--void-light);
    border: 1px solid var(--glass-border);
    border-radius: var(--space-sm);
    padding: var(--space-md);
    margin: var(--space-lg) 0;
    overflow-x: auto;
    position: relative;
}

.code-block::before {
    content: attr(data-label);
    position: absolute;
    top: -10px;
    left: 16px;
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.7rem;
    color: var(--money-green);
    background: var(--void);
    padding: 2px 10px;
    border-radius: 4px;
    border: 1px solid var(--glass-border);
}

.code-block pre {
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.85rem;
    line-height: 1.7;
    color: var(--text);
    white-space: pre;
    margin: 0;
}

.code-block .comment { color: var(--text-dim); }
.code-block .keyword { color: var(--money-gold); }
.code-block .string { color: var(--money-green); }
.code-block .function { color: var(--stock-cyan); }
.code-block .number { color: var(--profit-orange); }

/* Copy button */
.code-block .copy-btn,
.code-alive .copy-btn {
    position: absolute;
    top: 8px;
    right: 8px;
    background: rgba(255,255,255,0.08);
    border: 1px solid rgba(255,255,255,0.1);
    border-radius: 6px;
    padding: 6px 10px;
    color: var(--text-dim);
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.7rem;
    cursor: pointer;
    transition: all var(--dur-fast) var(--ease-smooth);
    display: flex;
    align-items: center;
    gap: 5px;
    z-index: 10;
}

.code-block .copy-btn:hover,
.code-alive .copy-btn:hover {
    background: var(--money-green);
    color: var(--void);
    border-color: var(--money-green);
    transform: scale(1.05);
}

.code-block .copy-btn.copied,
.code-alive .copy-btn.copied {
    background: var(--money-green);
    color: var(--void);
}

.code-block .copy-btn::before,
.code-alive .copy-btn::before {
    content: 'üìã';
    font-size: 0.9em;
}

.code-block .copy-btn.copied::before,
.code-alive .copy-btn.copied::before {
    content: '‚úì';
}

/* Line numbers */
.code-block .line-numbers {
    position: absolute;
    left: 0;
    top: var(--space-md);
    bottom: var(--space-md);
    width: 40px;
    padding-top: 0;
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.75rem;
    line-height: 1.7;
    color: rgba(255,255,255,0.2);
    text-align: right;
    padding-right: 12px;
    border-right: 1px solid var(--glass-border);
    user-select: none;
}

.code-block.has-lines pre {
    padding-left: 52px;
}

/* ‚ïê‚ïê‚ïê LIVE CODE BLOCK ‚ïê‚ïê‚ïê */
.code-alive {
    background: var(--void-light);
    border: none;
    border-radius: 16px;
    padding: 0;
    margin: var(--space-xl) 0;
    overflow: hidden;
    box-shadow: 0 4px 30px rgba(0, 0, 0, 0.4), 0 0 60px rgba(46, 204, 113, 0.08);
    position: relative;
}

.code-alive .terminal-header {
    background: linear-gradient(180deg, rgba(255, 255, 255, 0.08), rgba(255, 255, 255, 0.03));
    padding: 10px 16px;
    display: flex;
    align-items: center;
    gap: 8px;
    border-bottom: 1px solid rgba(255, 255, 255, 0.05);
}

.code-alive .terminal-dot {
    width: 12px;
    height: 12px;
    border-radius: 50%;
}

.code-alive .terminal-dot.red { background: #ff5f56; }
.code-alive .terminal-dot.yellow { background: #ffbd2e; }
.code-alive .terminal-dot.green { background: #27c93f; animation: dot-pulse 2s ease-in-out infinite; }

@keyframes dot-pulse {
    0%, 100% { box-shadow: 0 0 0 rgba(39, 201, 63, 0); }
    50% { box-shadow: 0 0 8px rgba(39, 201, 63, 0.6); }
}

.code-alive .terminal-title {
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.7rem;
    color: var(--text-dim);
    margin-left: 12px;
}

.code-alive .terminal-status {
    margin-left: auto;
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.65rem;
    color: var(--money-green);
    display: flex;
    align-items: center;
    gap: 6px;
}

.code-alive .terminal-status::before {
    content: '';
    width: 6px;
    height: 6px;
    background: var(--money-green);
    border-radius: 50%;
    animation: status-blink 1.5s ease-in-out infinite;
}

@keyframes status-blink {
    0%, 100% { opacity: 1; }
    50% { opacity: 0.3; }
}

.code-alive .copy-btn {
    top: 8px;
    right: 8px;
}

.code-alive pre { padding: var(--space-md) var(--space-lg); margin: 0; position: relative; }

.code-alive pre::after {
    content: '_';
    color: var(--money-green);
    font-weight: bold;
    animation: cursor-blink 0.8s step-end infinite;
}

@keyframes cursor-blink {
    0%, 50% { opacity: 1; }
    51%, 100% { opacity: 0; }
}

.code-alive::before {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    width: 3px;
    background: linear-gradient(180deg, var(--money-green) 0%, var(--money-gold) 50%, var(--stock-cyan) 100%);
    background-size: 100% 200%;
    animation: border-flow 3s ease-in-out infinite;
    z-index: 10;
}

@keyframes border-flow {
    0%, 100% { background-position: 0% 0%; }
    50% { background-position: 0% 100%; }
}

/* ‚ïê‚ïê‚ïê SKILL MARKETPLACE GRID ‚ïê‚ïê‚ïê */
.skills-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
    gap: var(--space-md);
    margin: var(--space-xl) 0;
}

.skill-card {
    background: var(--void-light);
    border: 1px solid var(--glass-border);
    border-radius: 16px;
    padding: var(--space-lg) var(--space-md);
    text-align: center;
    transition: all var(--dur-normal) var(--ease-bounce);
    cursor: default;
    position: relative;
    overflow: hidden;
}

.skill-card::before {
    content: '';
    position: absolute;
    inset: 0;
    background: linear-gradient(135deg, var(--money-green-glow), transparent);
    opacity: 0;
    transition: opacity var(--dur-normal);
}

.skill-card:hover {
    transform: translateY(-8px) scale(1.02);
    border-color: var(--money-green);
    box-shadow: 0 15px 40px rgba(46, 204, 113, 0.2);
}

.skill-card:hover::before { opacity: 1; }

.skill-card .emoji {
    font-size: 3rem;
    display: block;
    margin-bottom: var(--space-sm);
    filter: drop-shadow(0 4px 8px rgba(0, 0, 0, 0.3));
    transition: transform var(--dur-normal) var(--ease-bounce);
    position: relative;
}

.skill-card:hover .emoji { transform: scale(1.15) rotate(-5deg); }

.skill-card .label {
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.75rem;
    color: var(--text-dim);
    text-transform: uppercase;
    letter-spacing: 0.08em;
    display: block;
    margin-bottom: 4px;
}

.skill-card .skill-name {
    display: block;
    font-family: 'Bangers', cursive;
    font-size: 1.3rem;
    color: var(--money-green);
}

.skill-card .rate {
    display: block;
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.9rem;
    color: var(--money-gold);
    margin-top: 8px;
}

/* Proficiency bar */
.skill-card .proficiency {
    width: 100%;
    height: 4px;
    background: var(--void);
    border-radius: 2px;
    margin-top: 12px;
    overflow: hidden;
    position: relative;
}

.skill-card .proficiency-fill {
    height: 100%;
    background: linear-gradient(90deg, var(--money-green), var(--money-gold));
    border-radius: 2px;
    width: 0%;
    transition: width 1.2s var(--ease-money);
    position: relative;
}

.skill-card .proficiency-fill::after {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: linear-gradient(90deg, transparent, rgba(255,255,255,0.4), transparent);
    animation: shimmer 2s ease-in-out infinite;
}

@keyframes shimmer {
    0% { transform: translateX(-100%); }
    100% { transform: translateX(100%); }
}

.skill-card.visible .proficiency-fill {
    width: var(--proficiency);
}

/* Sparkle effect on hover */
.skill-card::after {
    content: '‚ú®';
    position: absolute;
    top: 10px;
    right: 10px;
    font-size: 0.9rem;
    opacity: 0;
    transform: scale(0) rotate(-20deg);
    transition: all var(--dur-normal) var(--ease-bounce);
}

.skill-card:hover::after {
    opacity: 1;
    transform: scale(1) rotate(0deg);
}

/* Skill level badge */
.skill-card .level {
    position: absolute;
    top: -8px;
    right: -8px;
    background: linear-gradient(135deg, var(--money-green), var(--money-green-dark));
    color: var(--void);
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.6rem;
    font-weight: 600;
    padding: 4px 8px;
    border-radius: 8px;
    text-transform: uppercase;
    letter-spacing: 0.05em;
    transform: scale(0);
    transition: transform var(--dur-normal) var(--ease-bounce) 0.1s;
}

.skill-card:hover .level {
    transform: scale(1);
}

/* ‚ïê‚ïê‚ïê ECONOMIC MODEL ‚ïê‚ïê‚ïê */
.model-section {
    background: var(--void-light);
    border: 1px solid var(--glass-border);
    border-radius: 20px;
    padding: var(--space-xl);
    margin: var(--space-xl) 0;
}

.model-equation {
    font-family: 'JetBrains Mono', monospace;
    font-size: 1.1rem;
    color: var(--stock-cyan);
    text-align: center;
    padding: var(--space-lg);
    background: rgba(0, 217, 255, 0.05);
    border-radius: 12px;
    margin: var(--space-md) 0;
    border: 1px solid rgba(0, 217, 255, 0.1);
}

.model-equation .var {
    color: var(--money-gold);
}

.model-equation .op {
    color: var(--text-dim);
}

/* ‚ïê‚ïê‚ïê PARETO FRONTIER ‚ïê‚ïê‚ïê */
.pareto-section {
    position: relative;
    padding: var(--space-xl) 0;
}

.pareto-chart {
    width: 100%;
    height: 300px;
    background: var(--void-light);
    border-radius: 16px;
    position: relative;
    overflow: hidden;
    margin: var(--space-lg) 0;
}

.pareto-chart .axis-label {
    position: absolute;
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.75rem;
    color: var(--text-dim);
}

.pareto-chart .axis-label.x {
    bottom: 10px;
    left: 50%;
    transform: translateX(-50%);
}

.pareto-chart .axis-label.y {
    left: 10px;
    top: 50%;
    transform: rotate(-90deg) translateX(-50%);
    transform-origin: left center;
}

/* ‚ïê‚ïê‚ïê BIG IDEA BOX ‚ïê‚ïê‚ïê */
.big-idea {
    background: linear-gradient(135deg, var(--money-green), var(--money-green-dark));
    color: var(--void);
    padding: var(--space-xl) var(--space-lg);
    border-radius: var(--space-md);
    text-align: center;
    margin: var(--space-xl) 0;
    position: relative;
    overflow: hidden;
    box-shadow: 0 var(--space-md) var(--space-xl) rgba(46, 204, 113, 0.3);
}

.big-idea::before {
    content: 'üíº';
    position: absolute;
    font-size: 8rem;
    opacity: 0.1;
    top: -20px;
    right: -20px;
    transform: rotate(15deg);
}

.big-idea p {
    font-family: 'Bangers', cursive;
    font-size: 1.8rem;
    color: var(--void);
    margin: 0;
    line-height: 1.4;
    position: relative;
    z-index: 1;
}

.big-idea .subtext {
    font-family: 'IBM Plex Sans', sans-serif;
    font-size: 1rem;
    margin-top: 16px;
    opacity: 0.8;
}

/* ‚ïê‚ïê‚ïê REVENUE METER ‚ïê‚ïê‚ïê */
.revenue-meter {
    background: var(--void-light);
    border-radius: 20px;
    padding: var(--space-lg);
    margin: var(--space-xl) 0;
    border: 1px solid var(--glass-border);
    position: relative;
    overflow: hidden;
}

.revenue-meter::before {
    content: '';
    position: absolute;
    top: 0;
    left: -100%;
    width: 200%;
    height: 100%;
    background: linear-gradient(90deg, 
        transparent 0%, 
        rgba(46, 204, 113, 0.03) 25%, 
        rgba(241, 196, 15, 0.03) 50%, 
        rgba(46, 204, 113, 0.03) 75%, 
        transparent 100%
    );
    animation: meter-sweep 4s linear infinite;
}

@keyframes meter-sweep {
    0% { transform: translateX(0); }
    100% { transform: translateX(50%); }
}

.meter-header {
    display: flex;
    justify-content: space-between;
    align-items: baseline;
    margin-bottom: var(--space-sm);
    position: relative;
}

.meter-title {
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.85rem;
    color: var(--money-gold);
    display: flex;
    align-items: center;
    gap: 8px;
}

.meter-title::before {
    content: 'üìä';
    font-size: 1rem;
}

.meter-value {
    font-family: 'JetBrains Mono', monospace;
    font-size: 1.5rem;
    font-weight: 600;
    color: var(--money-green);
    text-shadow: 0 0 20px var(--money-green-glow);
}

.meter-bar {
    height: 28px;
    background: var(--void);
    border-radius: 14px;
    overflow: hidden;
    position: relative;
    box-shadow: inset 0 2px 8px rgba(0,0,0,0.5);
}

.meter-fill {
    height: 100%;
    background: linear-gradient(90deg, var(--money-green), var(--money-gold), var(--profit-orange));
    background-size: 200% 100%;
    border-radius: 14px;
    width: 0;
    transition: width 1.5s var(--ease-money);
    position: relative;
    animation: meter-gradient 3s ease-in-out infinite;
}

@keyframes meter-gradient {
    0%, 100% { background-position: 0% 50%; }
    50% { background-position: 100% 50%; }
}

.meter-fill::after {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: linear-gradient(90deg, transparent 0%, rgba(255,255,255,0.3) 50%, transparent 100%);
    animation: meter-shimmer 2s ease-in-out infinite;
}

@keyframes meter-shimmer {
    0% { transform: translateX(-100%); }
    100% { transform: translateX(100%); }
}

.meter-markers {
    display: flex;
    justify-content: space-between;
    margin-top: var(--space-xs);
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.7rem;
    color: var(--text-dim);
    padding: 0 4px;
}

.meter-milestone {
    position: absolute;
    bottom: 100%;
    transform: translateX(-50%);
    margin-bottom: 8px;
    padding: 4px 8px;
    background: var(--money-gold);
    color: var(--void);
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.65rem;
    border-radius: 4px;
    white-space: nowrap;
    opacity: 0;
    transition: opacity var(--dur-normal);
}

.meter-milestone::after {
    content: '';
    position: absolute;
    top: 100%;
    left: 50%;
    transform: translateX(-50%);
    border: 5px solid transparent;
    border-top-color: var(--money-gold);
}

.revenue-meter:hover .meter-milestone {
    opacity: 1;
}

/* ‚ïê‚ïê‚ïê LEARNING LOOP ‚ïê‚ïê‚ïê */
.learning-loop {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    gap: var(--space-md);
    margin: var(--space-xl) 0;
    position: relative;
}

/* Animated connection line behind steps */
.learning-loop::before {
    content: '';
    position: absolute;
    top: 50%;
    left: 80px;
    right: 80px;
    height: 2px;
    background: linear-gradient(90deg, 
        var(--money-green) 0%, 
        var(--money-gold) 33%, 
        var(--stock-cyan) 66%, 
        var(--money-green) 100%
    );
    background-size: 300% 100%;
    animation: flow-gradient 3s ease-in-out infinite;
    z-index: 0;
    opacity: 0.3;
    transform: translateY(-50%);
}

@keyframes flow-gradient {
    0% { background-position: 0% 50%; }
    100% { background-position: 100% 50%; }
}

/* Data packet animation */
.learning-loop::after {
    content: '';
    position: absolute;
    top: 50%;
    left: 80px;
    width: 12px;
    height: 12px;
    background: var(--money-green);
    border-radius: 50%;
    transform: translateY(-50%);
    animation: data-packet 3s ease-in-out infinite;
    box-shadow: 0 0 15px var(--money-green);
    z-index: 1;
}

@keyframes data-packet {
    0% { left: 80px; opacity: 1; }
    25% { opacity: 1; }
    50% { opacity: 0.8; }
    75% { opacity: 1; }
    100% { left: calc(100% - 92px); opacity: 1; }
}

.loop-step {
    background: var(--void-light);
    z-index: 2;
    border: 1px solid var(--glass-border);
    border-radius: 50%;
    width: 120px;
    height: 120px;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    text-align: center;
    position: relative;
    transition: all var(--dur-normal) var(--ease-bounce);
}

.loop-step:hover {
    transform: scale(1.1);
    border-color: var(--money-green);
    box-shadow: 0 0 30px var(--money-green-glow);
}

.loop-step .step-icon {
    font-size: 2rem;
    margin-bottom: 4px;
}

.loop-step .step-label {
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.7rem;
    color: var(--text-dim);
    text-transform: uppercase;
}

.loop-arrow {
    display: flex;
    align-items: center;
    font-size: 1.5rem;
    color: var(--money-green);
    animation: arrow-pulse 1.5s ease-in-out infinite;
}

@keyframes arrow-pulse {
    0%, 100% { opacity: 0.5; transform: translateX(0); }
    50% { opacity: 1; transform: translateX(5px); }
}

/* ‚ïê‚ïê‚ïê CANVAS ‚ïê‚ïê‚ïê */
.canvas-container {
    position: relative;
    margin: var(--space-xl) 0;
    border-radius: var(--space-sm);
    overflow: hidden;
    box-shadow: 0 0 var(--space-xl) rgba(46, 204, 113, 0.1), inset 0 0 var(--space-xl) rgba(0,0,0,0.3);
}

#job-canvas {
    display: block;
    width: 100%;
    height: 400px;
    background: var(--void-light);
    cursor: pointer;
}

.canvas-hint {
    position: absolute;
    bottom: var(--space-sm);
    left: 50%;
    transform: translateX(-50%);
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.75rem;
    color: var(--text);
    background: rgba(0,0,0,0.8);
    padding: var(--space-xs) var(--space-md);
    border-radius: var(--space-md);
    pointer-events: none;
}

.canvas-hint .key {
    background: var(--money-green);
    color: var(--void);
    padding: 2px 8px;
    border-radius: 4px;
    font-weight: 600;
    margin: 0 4px;
}

/* ‚ïê‚ïê‚ïê SCROLL INDICATOR (MOUSE SHAPE) ‚ïê‚ïê‚ïê */
.scroll-indicator {
    position: absolute;
    bottom: var(--space-xl);
    left: 50%;
    transform: translateX(-50%);
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 12px;
    cursor: pointer;
    opacity: 0;
    animation: scroll-entrance 0.6s ease-out 1.2s both;
    transition: transform var(--dur-normal) var(--ease-smooth);
}

.scroll-indicator:hover {
    transform: translateX(-50%) scale(1.1);
}

@keyframes scroll-entrance {
    0% { opacity: 0; transform: translateX(-50%) translateY(20px); }
    100% { opacity: 1; transform: translateX(-50%) translateY(0); }
}

.scroll-mouse {
    width: 28px;
    height: 44px;
    border: 2px solid var(--money-green);
    border-radius: 14px;
    position: relative;
    opacity: 0.8;
    transition: all var(--dur-normal);
}

.scroll-indicator:hover .scroll-mouse {
    border-color: var(--money-gold);
    box-shadow: 0 0 20px var(--money-green-glow);
}

.scroll-wheel {
    position: absolute;
    top: 8px;
    left: 50%;
    transform: translateX(-50%);
    width: 4px;
    height: 10px;
    background: var(--money-green);
    border-radius: 2px;
    animation: scroll-wheel 1.5s ease-in-out infinite;
}

@keyframes scroll-wheel {
    0% { opacity: 1; transform: translateX(-50%) translateY(0); }
    50% { opacity: 0.3; transform: translateX(-50%) translateY(8px); }
    100% { opacity: 1; transform: translateX(-50%) translateY(0); }
}

.scroll-indicator .text {
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.65rem;
    letter-spacing: 0.2em;
    text-transform: uppercase;
    color: var(--text-dim);
    transition: color var(--dur-normal);
}

.scroll-indicator:hover .text {
    color: var(--money-green);
}

/* Money sparkle effect on hover */
.scroll-indicator::after {
    content: 'üíµ';
    position: absolute;
    top: -20px;
    opacity: 0;
    transform: scale(0);
    transition: all var(--dur-normal) var(--ease-bounce);
}

.scroll-indicator:hover::after {
    opacity: 1;
    transform: scale(1);
    animation: money-float 1s ease-out forwards;
}

@keyframes money-float {
    0% { transform: scale(1) translateY(0); opacity: 1; }
    100% { transform: scale(1.2) translateY(-30px); opacity: 0; }
}

/* ‚ïê‚ïê‚ïê FOOTER ‚ïê‚ïê‚ïê */
footer {
    text-align: center;
    padding: var(--space-2xl) var(--space-md) var(--space-xl);
    background: linear-gradient(180deg, var(--void) 0%, var(--void-light) 50%);
    border-top: 1px solid var(--glass-border);
    min-height: 60vh;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    position: relative;
    overflow: hidden;
}

footer::before {
    content: '';
    position: absolute;
    top: -50%;
    left: -50%;
    width: 200%;
    height: 200%;
    background: radial-gradient(ellipse at center, rgba(46, 204, 113, 0.05) 0%, transparent 50%);
    animation: footer-glow 8s ease-in-out infinite;
}

@keyframes footer-glow {
    0%, 100% { transform: scale(1); opacity: 0.5; }
    50% { transform: scale(1.2); opacity: 1; }
}

.footer-icon {
    font-size: 6rem;
    display: block;
    animation: work-hard 4s var(--ease-money) infinite;
    filter: drop-shadow(0 0 40px rgba(46, 204, 113, 0.6));
    margin-bottom: var(--space-lg);
    position: relative;
    z-index: 1;
}

@keyframes work-hard {
    0%, 100% { transform: rotate(0deg) scale(1); }
    25% { transform: rotate(-10deg) scale(1.05); }
    50% { transform: rotate(0deg) scale(1); }
    75% { transform: rotate(10deg) scale(1.02); }
}

.footer-tagline {
    font-family: 'Bangers', cursive;
    font-size: 3rem;
    color: var(--money-green);
    text-shadow: 4px 4px 0 var(--money-gold), 0 0 40px var(--money-green-glow);
    margin-bottom: var(--space-xs);
    position: relative;
    z-index: 1;
}

.footer-subtitle {
    font-size: 1.2rem;
    color: var(--text-dim);
    margin-bottom: var(--space-xl);
    position: relative;
    z-index: 1;
}

/* Stats grid */
.footer-stats {
    display: flex;
    gap: var(--space-xl);
    margin: var(--space-lg) 0;
    position: relative;
    z-index: 1;
}

.stat-item {
    text-align: center;
}

.stat-value {
    font-family: 'Bangers', cursive;
    font-size: 2.5rem;
    color: var(--money-green);
    text-shadow: 2px 2px 0 var(--money-gold);
    display: block;
}

.stat-label {
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.7rem;
    color: var(--text-dim);
    text-transform: uppercase;
    letter-spacing: 0.1em;
}

/* CTA button */
.footer-cta {
    display: inline-flex;
    align-items: center;
    gap: 10px;
    background: linear-gradient(135deg, var(--money-green), var(--money-green-dark));
    color: var(--void);
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.9rem;
    font-weight: 600;
    padding: 14px 28px;
    border-radius: 30px;
    text-decoration: none;
    margin-top: var(--space-lg);
    transition: all var(--dur-normal) var(--ease-bounce);
    box-shadow: 0 4px 20px var(--money-green-glow);
    position: relative;
    z-index: 1;
    cursor: pointer;
    border: none;
}

.footer-cta:hover {
    transform: translateY(-3px) scale(1.02);
    box-shadow: 0 8px 30px var(--money-green-glow);
}

.footer-cta::after {
    content: '‚Üí';
    transition: transform var(--dur-fast);
}

.footer-cta:hover::after {
    transform: translateX(5px);
}

/* ‚ïê‚ïê‚ïê ENTRANCE ANIMATIONS ‚ïê‚ïê‚ïê */
.fade-in {
    opacity: 0;
    transform: translateY(30px);
    transition: opacity var(--dur-dramatic) var(--ease-smooth), 
                transform var(--dur-dramatic) var(--ease-smooth);
}

.fade-in.visible {
    opacity: 1;
    transform: translateY(0);
}

/* Staggered children animations */
.stagger-children > * {
    opacity: 0;
    transform: translateY(20px);
    transition: opacity var(--dur-slow) var(--ease-smooth),
                transform var(--dur-slow) var(--ease-smooth);
}

.stagger-children.visible > *:nth-child(1) { transition-delay: 0ms; opacity: 1; transform: translateY(0); }
.stagger-children.visible > *:nth-child(2) { transition-delay: 80ms; opacity: 1; transform: translateY(0); }
.stagger-children.visible > *:nth-child(3) { transition-delay: 160ms; opacity: 1; transform: translateY(0); }
.stagger-children.visible > *:nth-child(4) { transition-delay: 240ms; opacity: 1; transform: translateY(0); }
.stagger-children.visible > *:nth-child(5) { transition-delay: 320ms; opacity: 1; transform: translateY(0); }
.stagger-children.visible > *:nth-child(6) { transition-delay: 400ms; opacity: 1; transform: translateY(0); }
.stagger-children.visible > *:nth-child(7) { transition-delay: 480ms; opacity: 1; transform: translateY(0); }
.stagger-children.visible > *:nth-child(8) { transition-delay: 560ms; opacity: 1; transform: translateY(0); }

/* Scale in animation */
.scale-in {
    opacity: 0;
    transform: scale(0.9);
    transition: opacity var(--dur-slow), transform var(--dur-slow) var(--ease-bounce);
}

.scale-in.visible {
    opacity: 1;
    transform: scale(1);
}

/* Slide in from sides */
.slide-in-left {
    opacity: 0;
    transform: translateX(-40px);
    transition: opacity var(--dur-slow), transform var(--dur-slow) var(--ease-smooth);
}

.slide-in-left.visible {
    opacity: 1;
    transform: translateX(0);
}

.slide-in-right {
    opacity: 0;
    transform: translateX(40px);
    transition: opacity var(--dur-slow), transform var(--dur-slow) var(--ease-smooth);
}

.slide-in-right.visible {
    opacity: 1;
    transform: translateX(0);
}

/* ‚ïê‚ïê‚ïê DELIGHT: SCROLL PROGRESS INDICATOR ‚ïê‚ïê‚ïê */
.scroll-journey {
    position: fixed;
    top: 0;
    left: 0;
    width: 0%;
    height: 3px;
    background: linear-gradient(90deg, var(--money-green), var(--money-gold), var(--stock-cyan));
    z-index: 10000;
    transition: width 50ms linear;
}

.scroll-journey::after {
    content: '';
    position: absolute;
    right: 0;
    top: 0;
    width: 20px;
    height: 100%;
    background: linear-gradient(90deg, transparent, rgba(255,255,255,0.8));
    filter: blur(3px);
}

/* Scroll milestone popups */
.scroll-milestone {
    position: fixed;
    bottom: 80px;
    right: 20px;
    background: linear-gradient(135deg, var(--void-light), var(--void));
    border: 1px solid var(--money-green);
    border-radius: 12px;
    padding: 12px 20px;
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.85rem;
    color: var(--money-green);
    z-index: 9999;
    opacity: 0;
    transform: translateX(100px);
    transition: all var(--dur-slow) var(--ease-bounce);
    pointer-events: none;
    box-shadow: 0 4px 20px rgba(46, 204, 113, 0.3);
}

.scroll-milestone.show {
    opacity: 1;
    transform: translateX(0);
}

.scroll-milestone .emoji {
    font-size: 1.5rem;
    margin-right: 10px;
}

/* ‚ïê‚ïê‚ïê DELIGHT: SKILL CARD HIGH-FIVE ‚ïê‚ïê‚ïê */
.skill-card.high-five {
    animation: high-five-shake 0.4s var(--ease-bounce);
}

@keyframes high-five-shake {
    0% { transform: translateY(-8px) scale(1.02) rotate(0deg); }
    25% { transform: translateY(-10px) scale(1.05) rotate(-3deg); }
    50% { transform: translateY(-12px) scale(1.08) rotate(3deg); }
    75% { transform: translateY(-10px) scale(1.05) rotate(-2deg); }
    100% { transform: translateY(-8px) scale(1.02) rotate(0deg); }
}

.skill-card .high-five-text {
    position: absolute;
    top: -30px;
    left: 50%;
    transform: translateX(-50%);
    font-family: 'Bangers', cursive;
    font-size: 1rem;
    color: var(--money-gold);
    opacity: 0;
    pointer-events: none;
    white-space: nowrap;
    text-shadow: 0 0 10px var(--money-gold);
}

.skill-card.high-five .high-five-text {
    animation: high-five-pop 0.6s var(--ease-bounce) forwards;
}

@keyframes high-five-pop {
    0% { opacity: 0; transform: translateX(-50%) translateY(10px) scale(0.5); }
    50% { opacity: 1; transform: translateX(-50%) translateY(-5px) scale(1.2); }
    100% { opacity: 0; transform: translateX(-50%) translateY(-20px) scale(1); }
}

/* ‚ïê‚ïê‚ïê DELIGHT: KONAMI CODE SECRET ‚ïê‚ïê‚ïê */
.konami-overlay {
    position: fixed;
    inset: 0;
    background: rgba(10, 14, 19, 0.98);
    z-index: 100000;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    opacity: 0;
    visibility: hidden;
    transition: all var(--dur-slow) var(--ease-smooth);
}

.konami-overlay.show {
    opacity: 1;
    visibility: visible;
}

.konami-content {
    text-align: center;
    max-width: 600px;
    padding: 40px;
    transform: scale(0.8);
    transition: transform var(--dur-dramatic) var(--ease-bounce);
}

.konami-overlay.show .konami-content {
    transform: scale(1);
}

.konami-emoji {
    font-size: 6rem;
    animation: konami-bounce 1s var(--ease-bounce) infinite;
    filter: drop-shadow(0 0 30px var(--money-green-glow));
}

@keyframes konami-bounce {
    0%, 100% { transform: translateY(0) rotate(-5deg); }
    50% { transform: translateY(-20px) rotate(5deg); }
}

.konami-quote {
    font-family: 'Bangers', cursive;
    font-size: 2rem;
    color: var(--money-green);
    text-shadow: 3px 3px 0 var(--money-gold);
    margin: 30px 0 20px;
    line-height: 1.4;
}

.konami-subtext {
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.9rem;
    color: var(--text-dim);
    margin-bottom: 30px;
}

.konami-hint {
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.75rem;
    color: var(--text-dim);
    opacity: 0.5;
}

/* ‚ïê‚ïê‚ïê DELIGHT: ACHIEVEMENT UNLOCK ‚ïê‚ïê‚ïê */
.achievement-toast {
    position: fixed;
    top: 80px;
    left: 50%;
    transform: translateX(-50%) translateY(-100px);
    background: linear-gradient(135deg, var(--void-light), var(--void));
    border: 2px solid var(--money-gold);
    border-radius: 16px;
    padding: 16px 24px;
    z-index: 99999;
    display: flex;
    align-items: center;
    gap: 16px;
    opacity: 0;
    transition: all var(--dur-slow) var(--ease-bounce);
    box-shadow: 0 8px 40px rgba(241, 196, 15, 0.3), 0 0 60px rgba(241, 196, 15, 0.1);
}

.achievement-toast.show {
    opacity: 1;
    transform: translateX(-50%) translateY(0);
}

.achievement-icon {
    font-size: 2.5rem;
    animation: achievement-spin 1s var(--ease-bounce);
}

@keyframes achievement-spin {
    0% { transform: scale(0) rotate(-180deg); }
    50% { transform: scale(1.2) rotate(10deg); }
    100% { transform: scale(1) rotate(0deg); }
}

.achievement-text {
    text-align: left;
}

.achievement-label {
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.65rem;
    color: var(--money-gold);
    text-transform: uppercase;
    letter-spacing: 0.1em;
}

.achievement-title {
    font-family: 'Bangers', cursive;
    font-size: 1.2rem;
    color: var(--text);
}

/* ‚ïê‚ïê‚ïê DELIGHT: VIDEO FALLBACK ‚ïê‚ïê‚ïê */
.video-fallback {
    background: var(--void-light);
    border: 2px dashed var(--glass-border);
    border-radius: 16px;
    padding: 40px;
    text-align: center;
    display: none;
}

.video-fallback.show {
    display: block;
    animation: fallback-appear 0.5s var(--ease-smooth);
}

@keyframes fallback-appear {
    0% { opacity: 0; transform: scale(0.95); }
    100% { opacity: 1; transform: scale(1); }
}

.video-fallback .fallback-emoji {
    font-size: 4rem;
    animation: fallback-shrug 2s ease-in-out infinite;
}

@keyframes fallback-shrug {
    0%, 100% { transform: translateY(0); }
    50% { transform: translateY(-10px); }
}

.video-fallback .fallback-text {
    font-family: 'Bangers', cursive;
    font-size: 1.5rem;
    color: var(--text-dim);
    margin: 16px 0 8px;
}

.video-fallback .fallback-subtext {
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.8rem;
    color: var(--text-dim);
    opacity: 0.7;
}


/* ‚ïê‚ïê‚ïê TOOLTIP ‚ïê‚ïê‚ïê */
[data-tooltip] {
    position: relative;
    cursor: help;
}

[data-tooltip]::after {
    content: attr(data-tooltip);
    position: absolute;
    bottom: 100%;
    left: 50%;
    transform: translateX(-50%) translateY(-5px);
    background: var(--void);
    color: var(--text);
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.75rem;
    padding: 8px 12px;
    border-radius: 8px;
    white-space: nowrap;
    opacity: 0;
    visibility: hidden;
    transition: all var(--dur-fast);
    box-shadow: 0 4px 20px rgba(0,0,0,0.5);
    border: 1px solid var(--glass-border);
    z-index: 100;
}

[data-tooltip]:hover::after {
    opacity: 1;
    visibility: visible;
    transform: translateX(-50%) translateY(-10px);
}

/* ‚ïê‚ïê‚ïê REDUCED MOTION ‚ïê‚ïê‚ïê */
@media (prefers-reduced-motion: reduce) {
    *, *::before, *::after {
        animation-duration: 0.01ms !important;
        animation-iteration-count: 1 !important;
        transition-duration: 0.01ms !important;
    }
    
    .preloader { display: none; }
}

/* ‚ïê‚ïê‚ïê FOCUS STATES ‚ïê‚ïê‚ïê */
*:focus-visible {
    outline: 2px solid var(--money-green);
    outline-offset: 3px;
}

/* ‚ïê‚ïê‚ïê MOBILE RESPONSIVE ‚ïê‚ïê‚ïê */

/* ‚ïê‚ïê‚ïê VIDEO PRESENCE GRID ‚ïê‚ïê‚ïê */
.presence-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(140px, 1fr));
    gap: var(--space-md);
    margin: var(--space-lg) 0;
}

.presence-card {
    background: var(--void-light);
    border: 1px solid var(--glass-border);
    border-radius: 12px;
    padding: var(--space-md);
    text-align: center;
    transition: all var(--dur-normal) var(--ease-bounce);
}

.presence-card:hover {
    transform: translateY(-4px);
    border-color: var(--stock-cyan);
    box-shadow: 0 8px 25px rgba(0, 217, 255, 0.15);
}

.presence-card .presence-icon {
    font-size: 2rem;
    display: block;
    margin-bottom: var(--space-xs);
}

.presence-card .presence-label {
    display: block;
    font-family: 'Bangers', cursive;
    font-size: 1.1rem;
    color: var(--stock-cyan);
}

.presence-card .presence-desc {
    display: block;
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.7rem;
    color: var(--text-dim);
    margin-top: 4px;
}

/* ‚ïê‚ïê‚ïê VOICE DEMO ‚ïê‚ïê‚ïê */
.voice-demo {
    background: var(--void-light);
    border: 1px solid var(--glass-border);
    border-radius: 16px;
    padding: var(--space-lg);
    margin: var(--space-lg) 0;
}

.voice-waveform {
    height: 60px;
    background: linear-gradient(90deg, 
        rgba(46, 204, 113, 0.1) 0%, 
        rgba(0, 217, 255, 0.2) 50%, 
        rgba(46, 204, 113, 0.1) 100%
    );
    border-radius: 8px;
    margin-bottom: var(--space-md);
    position: relative;
    overflow: hidden;
}

.voice-waveform::after {
    content: '';
    position: absolute;
    inset: 0;
    background: repeating-linear-gradient(
        90deg,
        transparent 0px,
        transparent 2px,
        rgba(0, 217, 255, 0.3) 2px,
        rgba(0, 217, 255, 0.3) 4px
    );
    animation: waveform-scroll 2s linear infinite;
}

@keyframes waveform-scroll {
    0% { transform: translateX(0); }
    100% { transform: translateX(-8px); }
}

.voice-controls {
    display: flex;
    align-items: center;
    gap: var(--space-md);
}

.voice-btn {
    display: flex;
    align-items: center;
    gap: 8px;
    background: linear-gradient(135deg, var(--stock-cyan), var(--market-blue));
    color: var(--void);
    border: none;
    border-radius: 25px;
    padding: 10px 20px;
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.85rem;
    font-weight: 600;
    cursor: pointer;
    transition: all var(--dur-normal) var(--ease-bounce);
}

.voice-btn:hover {
    transform: scale(1.05);
    box-shadow: 0 4px 20px rgba(0, 217, 255, 0.4);
}

.voice-persona {
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.75rem;
    color: var(--text-dim);
}

/* ‚ïê‚ïê‚ïê PIPELINE FLOW ‚ïê‚ïê‚ïê */
.pipeline-flow {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    align-items: center;
    gap: var(--space-sm);
    margin: var(--space-xl) 0;
    padding: var(--space-md);
    background: var(--void-light);
    border-radius: 16px;
}

.pipeline-step {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 4px;
    padding: var(--space-sm);
}

.pipeline-step .pipeline-icon {
    font-size: 1.8rem;
}

.pipeline-step .pipeline-label {
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.7rem;
    color: var(--text-dim);
    text-transform: uppercase;
}

.pipeline-arrow {
    color: var(--money-green);
    font-size: 1.2rem;
    animation: arrow-pulse 1.5s ease-in-out infinite;
}

/* ‚ïê‚ïê‚ïê JOB SOURCES GRID ‚ïê‚ïê‚ïê */
.job-sources-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(160px, 1fr));
    gap: var(--space-md);
    margin: var(--space-lg) 0;
}

.job-source {
    background: var(--void-light);
    border: 1px solid var(--glass-border);
    border-radius: 12px;
    padding: var(--space-md);
    text-align: center;
    transition: all var(--dur-normal) var(--ease-smooth);
    opacity: 0.6;
}

.job-source.active {
    opacity: 1;
    border-color: var(--money-green);
}

.job-source:hover {
    transform: translateY(-2px);
}

.job-source .source-icon {
    font-size: 2rem;
    display: block;
    margin-bottom: var(--space-xs);
}

.job-source .source-name {
    display: block;
    font-family: 'Bangers', cursive;
    font-size: 1.1rem;
    color: var(--text);
}

.job-source .source-status {
    display: block;
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.7rem;
    color: var(--money-green);
    margin-top: 4px;
}

.job-source:not(.active) .source-status {
    color: var(--text-dim);
}

.job-source .source-jobs {
    display: block;
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.75rem;
    color: var(--money-gold);
    margin-top: 4px;
}

/* ‚ïê‚ïê‚ïê MATCH ANALYSIS ‚ïê‚ïê‚ïê */
.match-analysis {
    background: var(--void-light);
    border: 1px solid var(--glass-border);
    border-radius: 16px;
    padding: var(--space-lg);
    margin: var(--space-lg) 0;
}

.match-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: var(--space-md);
    padding-bottom: var(--space-sm);
    border-bottom: 1px solid var(--glass-border);
}

.match-job-title {
    font-family: 'Bangers', cursive;
    font-size: 1.3rem;
    color: var(--money-green);
}

.match-score {
    font-family: 'JetBrains Mono', monospace;
    font-size: 1rem;
    color: var(--money-gold);
    background: rgba(241, 196, 15, 0.1);
    padding: 4px 12px;
    border-radius: 20px;
}

.match-requirements {
    margin: var(--space-md) 0;
}

.req-item {
    display: flex;
    align-items: center;
    gap: var(--space-sm);
    padding: 8px 0;
    border-bottom: 1px solid rgba(255,255,255,0.03);
}

.req-item .req-icon {
    width: 24px;
    text-align: center;
}

.req-item.matched .req-icon {
    color: var(--money-green);
}

.req-item.partial .req-icon {
    color: var(--money-gold);
}

.req-item .req-text {
    flex: 1;
    color: var(--text);
}

.req-item .req-confidence {
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.8rem;
    color: var(--text-dim);
}

.match-recommendation {
    display: flex;
    align-items: center;
    gap: var(--space-md);
    margin-top: var(--space-md);
    padding-top: var(--space-md);
    border-top: 1px solid var(--glass-border);
}

.rec-label {
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.8rem;
    color: var(--text-dim);
}

.rec-action {
    font-family: 'Bangers', cursive;
    font-size: 1.1rem;
    padding: 4px 12px;
    border-radius: 6px;
}

.rec-action.bid {
    background: var(--money-green);
    color: var(--void);
}

.rec-amount {
    font-family: 'JetBrains Mono', monospace;
    font-size: 1rem;
    color: var(--money-gold);
    margin-left: auto;
}

/* ‚ïê‚ïê‚ïê VALUE METRICS ‚ïê‚ïê‚ïê */
.value-metrics {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(140px, 1fr));
    gap: var(--space-md);
    margin: var(--space-lg) 0;
}

.metric-card {
    background: var(--void-light);
    border: 1px solid var(--glass-border);
    border-radius: 16px;
    padding: var(--space-lg) var(--space-md);
    text-align: center;
    transition: all var(--dur-normal) var(--ease-bounce);
}

.metric-card:hover {
    transform: translateY(-4px);
    border-color: var(--money-green);
    box-shadow: 0 8px 25px rgba(46, 204, 113, 0.15);
}

.metric-card .metric-icon {
    font-size: 1.5rem;
    display: block;
    margin-bottom: var(--space-xs);
}

.metric-card .metric-value {
    display: block;
    font-family: 'Bangers', cursive;
    font-size: 2rem;
    color: var(--money-green);
    text-shadow: 2px 2px 0 var(--money-gold);
}

.metric-card .metric-unit {
    display: block;
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.7rem;
    color: var(--text-dim);
}

.metric-card .metric-label {
    display: block;
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.75rem;
    color: var(--money-gold);
    margin-top: var(--space-xs);
    text-transform: uppercase;
    letter-spacing: 0.05em;
}

/* ‚ïê‚ïê‚ïê CAPABILITY ASSESSMENT ‚ïê‚ïê‚ïê */
.capability-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
    gap: var(--space-md);
    margin: var(--space-lg) 0;
}

.capability-item {
    background: var(--void-light);
    border: 1px solid var(--glass-border);
    border-radius: 12px;
    padding: var(--space-md);
    display: grid;
    grid-template-columns: auto 1fr auto;
    grid-template-rows: auto auto;
    gap: 4px var(--space-sm);
    align-items: center;
    transition: all var(--dur-normal) var(--ease-smooth);
}

.capability-item:hover {
    transform: translateY(-2px);
}

.capability-item.can-do {
    border-left: 4px solid var(--money-green);
}

.capability-item.partial {
    border-left: 4px solid var(--money-gold);
}

.capability-item.cannot-do {
    border-left: 4px solid var(--loss-red);
    opacity: 0.8;
}

.capability-item .cap-icon {
    font-size: 1.5rem;
    grid-row: span 2;
}

.capability-item .cap-name {
    font-family: 'Bangers', cursive;
    font-size: 1.1rem;
    color: var(--text);
}

.capability-item .cap-level {
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.7rem;
    padding: 2px 8px;
    border-radius: 4px;
    text-transform: uppercase;
    justify-self: end;
}

.capability-item.can-do .cap-level {
    background: rgba(46, 204, 113, 0.2);
    color: var(--money-green);
}

.capability-item.partial .cap-level {
    background: rgba(241, 196, 15, 0.2);
    color: var(--money-gold);
}

.capability-item.cannot-do .cap-level {
    background: rgba(231, 76, 60, 0.2);
    color: var(--loss-red);
}

.capability-item .cap-evidence {
    grid-column: 2 / 4;
    font-size: 0.8rem;
    color: var(--text-dim);
}

/* ‚ïê‚ïê‚ïê ASSESSMENT MATRIX ‚ïê‚ïê‚ïê */
.assessment-matrix {
    background: var(--void-light);
    border: 1px solid var(--glass-border);
    border-radius: 16px;
    overflow: hidden;
    margin: var(--space-lg) 0;
}

.matrix-header {
    display: grid;
    grid-template-columns: 2fr 1fr 2fr;
    gap: var(--space-md);
    padding: var(--space-md);
    background: rgba(255, 255, 255, 0.03);
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.75rem;
    color: var(--text-dim);
    text-transform: uppercase;
    letter-spacing: 0.05em;
}

.matrix-row {
    display: grid;
    grid-template-columns: 2fr 1fr 2fr;
    gap: var(--space-md);
    padding: var(--space-sm) var(--space-md);
    border-top: 1px solid var(--glass-border);
    transition: background var(--dur-fast);
}

.matrix-row:hover {
    background: rgba(255, 255, 255, 0.02);
}

.matrix-row.good .score { color: var(--money-green); }
.matrix-row.moderate .score { color: var(--money-gold); }
.matrix-row.poor .score { color: var(--profit-orange); }
.matrix-row.reject .score { color: var(--loss-red); }

.matrix-cell {
    font-size: 0.85rem;
    color: var(--text);
}

.matrix-cell.score {
    font-family: 'JetBrains Mono', monospace;
    font-weight: 600;
}

.matrix-cell:last-child {
    color: var(--text-dim);
    font-size: 0.8rem;
}

/* ‚ïê‚ïê‚ïê PHONE RESPONSIVE (480px and below) ‚ïê‚ïê‚ïê */
@media (max-width: 480px) {
