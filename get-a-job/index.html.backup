// CROSS-BROWSER CANVAS HELPERS
// ‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê

/**
 * Draw a rounded rectangle using arc/lineTo for Safari compatibility.
 * ctx.roundRect() is not supported in Safari.
 */
function drawRoundedRect(ctx, x, y, w, h, r) {
    if (typeof r === 'number') {
        r = { tl: r, tr: r, br: r, bl: r };
    }
    ctx.beginPath();
    ctx.moveTo(x + r.tl, y);
    ctx.lineTo(x + w - r.tr, y);
    ctx.arc(x + w - r.tr, y + r.tr, r.tr, -Math.PI / 2, 0);
    ctx.lineTo(x + w, y + h - r.br);
    ctx.arc(x + w - r.br, y + h - r.br, r.br, 0, Math.PI / 2);
    ctx.lineTo(x + r.bl, y + h);
    ctx.arc(x + r.bl, y + h - r.bl, r.bl, Math.PI / 2, Math.PI);
    ctx.lineTo(x, y + r.tl);
    ctx.arc(x + r.tl, y + r.tl, r.tl, Math.PI, Math.PI * 1.5);
    ctx.closePath();
}

