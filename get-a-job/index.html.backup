// Expose economic API for console exploration
window.kagamiEconomic = {
    getStatus: () => ({
        skills: ['Python', 'Machine Learning', 'Data Science', 'API Development', 'Technical Writing', 'Automation'],
        markets: ['Freelancer.com', 'Plugin Marketplace', 'Stripe'],
        dailyTarget: 50,
        currentRevenue: 16.00,
        capacityPercent: 70,
        riskTolerance: 0.35,
    }),
    calculateEFE: (revenue, risk, cost, infoGain) => {
        const lambda = 0.35;
        return revenue - (lambda * risk) - cost + infoGain;
    },
    simulateBid: (jobValue, winProbability) => {
        const efe = window.kagamiEconomic.calculateEFE(
            jobValue * winProbability,
            jobValue * (1 - winProbability),
            5, // API cost
            10  // Learning value
        );
        return { jobValue, winProbability, efe, recommendation: efe > 50 ? 'BID' : 'PASS' };
    }
};
