    height: auto;
    opacity: 0.25;
    filter: grayscale(10%);
    border-radius: 12px;
    box-shadow: 0 8px 32px rgba(46, 204, 113, 0.15);
    transition: all var(--dur-normal) var(--ease-smooth);
    pointer-events: none;
}

.video-container:hover .pitch-portrait {
