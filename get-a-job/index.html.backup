        }
    }

    resize() {
        const container = this.canvas.parentElement;
        this.canvas.width = container.clientWidth;
        this.canvas.height = 400;
    }

    generateJobs() {
        const types = [
            { name: 'Python Script', skill: 'python', baseRevenue: 150, baseRisk: 0.2 },
            { name: 'ML Model', skill: 'ml', baseRevenue: 400, baseRisk: 0.4 },
            { name: 'API Integration', skill: 'api', baseRevenue: 200, baseRisk: 0.25 },
            { name: 'Data Analysis', skill: 'data', baseRevenue: 250, baseRisk: 0.3 },
            { name: 'Automation', skill: 'automation', baseRevenue: 180, baseRisk: 0.2 },
            { name: 'Tech Writing', skill: 'writing', baseRevenue: 100, baseRisk: 0.15 },
        ];

        this.jobs = [];
        for (let i = 0; i < 20; i++) {
            const type = types[Math.floor(Math.random() * types.length)];
            const revenue = type.baseRevenue * (0.5 + Math.random());
            const risk = type.baseRisk * (0.5 + Math.random());
            const efe = revenue * (1 - risk) - 20 + Math.random() * 50; // Simplified EFE
            
            this.jobs.push({
                id: i,
                name: `${type.name} #${i + 1}`,
                skill: type.skill,
                revenue: revenue,
                risk: risk,
                efe: efe,
                x: 0,
                y: 0,
                radius: 8 + efe / 30,
                color: this.getJobColor(efe),
                selected: false,
            });
        }

        // Position jobs on the chart
        const padding = 60;
        const chartWidth = this.canvas.width - padding * 2;
        const chartHeight = this.canvas.height - padding * 2;

        this.jobs.forEach(job => {
            // X = revenue (normalized), Y = risk (inverted)
            job.x = padding + (job.revenue / 500) * chartWidth;
            job.y = padding + (1 - job.risk) * chartHeight;
        });
    }

    getJobColor(efe) {
        if (efe > 200) return '#2ECC71'; // High EFE - green
        if (efe > 100) return '#F1C40F'; // Medium EFE - gold
        return '#E67E22'; // Low EFE - orange
    }

    calculateParetoFrontier() {
        // Sort by revenue descending
        const sorted = [...this.jobs].sort((a, b) => b.revenue - a.revenue);
        
        this.paretoFrontier = [];
        let minRisk = 1;

        for (const job of sorted) {
            if (job.risk < minRisk) {
                this.paretoFrontier.push(job);
                minRisk = job.risk;
            }
        }
    }

    bindEvents() {
        window.addEventListener('resize', () => {
            this.resize();
            this.generateJobs();
            this.calculateParetoFrontier();
        });

        this.canvas.addEventListener('mousemove', (e) => {
            const rect = this.canvas.getBoundingClientRect();
            const x = e.clientX - rect.left;
            const y = e.clientY - rect.top;

            this.hoveredJob = null;
            for (const job of this.jobs) {
                const dist = Math.sqrt((x - job.x) ** 2 + (y - job.y) ** 2);
                if (dist < job.radius + 5) {
                    this.hoveredJob = job;
                    break;
                }
            }
        });

        this.canvas.addEventListener('click', () => {
            if (this.hoveredJob) {
                this.hoveredJob.selected = !this.hoveredJob.selected;
            }
        });
    }

    animate() {
        // Pause when tab is hidden
        if (!AnimationController.isVisible) {
            this.animationId = null;
            return;
        }

        const ctx = this.ctx;
        const width = this.canvas.width;
        const height = this.canvas.height;

        // Clear
        ctx.fillStyle = '#111820';
        ctx.fillRect(0, 0, width, height);

        // Draw grid
        ctx.strokeStyle = 'rgba(46, 204, 113, 0.1)';
        ctx.lineWidth = 1;
        for (let i = 0; i <= 10; i++) {
            const x = 60 + (i / 10) * (width - 120);
            const y = 60 + (i / 10) * (height - 120);

            ctx.beginPath();
            ctx.moveTo(x, 60);
            ctx.lineTo(x, height - 60);
            ctx.stroke();

            ctx.beginPath();
            ctx.moveTo(60, y);
            ctx.lineTo(width - 60, y);
            ctx.stroke();
        }

        // Draw Pareto frontier with smooth curve
        if (this.paretoFrontier.length > 1) {
            // Draw glow behind the curve
            ctx.beginPath();
            ctx.strokeStyle = 'rgba(46, 204, 113, 0.15)';
            ctx.lineWidth = 12;
            ctx.lineCap = 'round';
            ctx.lineJoin = 'round';

            this.drawSmoothCurve(ctx, this.paretoFrontier);
            ctx.stroke();

            // Draw the main curve
            ctx.beginPath();
            ctx.strokeStyle = 'rgba(46, 204, 113, 0.7)';
            ctx.lineWidth = 3;

            this.drawSmoothCurve(ctx, this.paretoFrontier);
            ctx.stroke();

            // Draw animated dash overlay
            const time = Date.now() / 1000;
            ctx.beginPath();
            ctx.strokeStyle = 'rgba(241, 196, 15, 0.6)';
            ctx.lineWidth = 2;
            ctx.setLineDash([10, 20]);
            ctx.lineDashOffset = -time * 30;

            this.drawSmoothCurve(ctx, this.paretoFrontier);
            ctx.stroke();
            ctx.setLineDash([]);
        }

        // Draw jobs
        this.drawJobs(ctx);

        // Draw tooltip and axis labels
        this.drawTooltipAndLabels(ctx, width, height);

        this.animationId = requestAnimationFrame(() => this.animate());
    }

    drawJobs(ctx) {
        // Draw jobs with breathing effect
        const time = Date.now() / 1000;

        for (const job of this.jobs) {
            const isHovered = job === this.hoveredJob;
            const isPareto = this.paretoFrontier.includes(job);

            // Breathing animation offset per job
            const breatheOffset = Math.sin(time * 2 + job.id * 0.5) * 0.15;
            const currentRadius = job.radius * (1 + breatheOffset);

            // Glow for Pareto optimal (pulsing)
            if (isPareto) {
                const pulseSize = 8 + Math.sin(time * 3 + job.id) * 3;
                const pulseOpacity = 0.15 + Math.sin(time * 2 + job.id) * 0.1;

                ctx.beginPath();
                ctx.arc(job.x, job.y, currentRadius + pulseSize, 0, Math.PI * 2);
                ctx.fillStyle = `rgba(46, 204, 113, ${pulseOpacity})`;
                ctx.fill();

                // Inner glow ring
                ctx.beginPath();
                ctx.arc(job.x, job.y, currentRadius + 3, 0, Math.PI * 2);
                ctx.strokeStyle = 'rgba(46, 204, 113, 0.4)';
                ctx.lineWidth = 2;
                ctx.stroke();
            }

            // Job circle with gradient
            const gradient = ctx.createRadialGradient(
                job.x - currentRadius * 0.3, job.y - currentRadius * 0.3, 0,
                job.x, job.y, currentRadius * 1.2
            );
            gradient.addColorStop(0, this.lightenColor(job.color, 30));
            gradient.addColorStop(1, job.color);

            ctx.beginPath();
            ctx.arc(job.x, job.y, isHovered ? currentRadius + 6 : currentRadius, 0, Math.PI * 2);
            ctx.fillStyle = gradient;
            ctx.fill();

            // Hover ring effect
            if (isHovered) {
                ctx.strokeStyle = 'rgba(255, 255, 255, 0.8)';
                ctx.lineWidth = 2;
                ctx.stroke();

                // Outer pulse ring
                ctx.beginPath();
                ctx.arc(job.x, job.y, currentRadius + 12, 0, Math.PI * 2);
                ctx.strokeStyle = 'rgba(255, 255, 255, 0.2)';
                ctx.lineWidth = 1;
                ctx.stroke();
            }

            if (job.selected) {
                ctx.beginPath();
                ctx.arc(job.x, job.y, currentRadius + 3, 0, Math.PI * 2);
                ctx.strokeStyle = '#fff';
                ctx.lineWidth = 3;
                ctx.stroke();

                // Selection checkmark indicator
                ctx.fillStyle = '#fff';
                ctx.font = 'bold 10px sans-serif';
                ctx.fillText('‚úì', job.x - 4, job.y + 3);
            }
        }
    }

    drawSmoothCurve(ctx, points) {
        if (points.length < 2) return;

        ctx.moveTo(points[0].x, points[0].y);

        for (let i = 0; i < points.length - 1; i++) {
            const p0 = points[Math.max(i - 1, 0)];
            const p1 = points[i];
            const p2 = points[i + 1];
            const p3 = points[Math.min(i + 2, points.length - 1)];

            // Catmull-Rom to Bezier conversion
            const cp1x = p1.x + (p2.x - p0.x) / 6;
            const cp1y = p1.y + (p2.y - p0.y) / 6;
            const cp2x = p2.x - (p3.x - p1.x) / 6;
            const cp2y = p2.y - (p3.y - p1.y) / 6;

            ctx.bezierCurveTo(cp1x, cp1y, cp2x, cp2y, p2.x, p2.y);
        }
    }

    lightenColor(color, percent) {
        const num = parseInt(color.replace('#', ''), 16);
        const amt = Math.round(2.55 * percent);
        const R = Math.min(255, (num >> 16) + amt);
        const G = Math.min(255, ((num >> 8) & 0x00FF) + amt);
        const B = Math.min(255, (num & 0x0000FF) + amt);
        return `rgb(${R}, ${G}, ${B})`;
    }

    drawTooltipAndLabels(ctx, width, height) {
        // Draw enhanced tooltip with glassmorphism
        if (this.hoveredJob) {
            const job = this.hoveredJob;
            const tooltipWidth = 190;
            const tooltipHeight = 100;
            const tooltipX = Math.min(job.x + 20, width - tooltipWidth - 10);
            const tooltipY = Math.max(job.y - tooltipHeight/2, 10);
            const isPareto = this.paretoFrontier.includes(job);

            // Shadow
            ctx.shadowColor = 'rgba(0, 0, 0, 0.5)';
            ctx.shadowBlur = 20;
            ctx.shadowOffsetX = 0;
            ctx.shadowOffsetY = 10;

            // Background (using cross-browser helper for Safari compatibility)
            ctx.fillStyle = 'rgba(10, 14, 19, 0.95)';
            drawRoundedRect(ctx, tooltipX, tooltipY, tooltipWidth, tooltipHeight, 12);
            ctx.fill();

            // Reset shadow
            ctx.shadowColor = 'transparent';
            ctx.shadowBlur = 0;

            // Border glow
            ctx.strokeStyle = isPareto ? 'rgba(46, 204, 113, 0.5)' : 'rgba(255, 255, 255, 0.1)';
            ctx.lineWidth = 1;
            ctx.stroke();

            // Pareto badge (using cross-browser helper for Safari compatibility)
            if (isPareto) {
                ctx.fillStyle = '#2ECC71';
                drawRoundedRect(ctx, tooltipX + tooltipWidth - 60, tooltipY + 8, 50, 16, 8);
                ctx.fill();
                ctx.fillStyle = '#0A0E13';
                ctx.font = 'bold 8px "JetBrains Mono", monospace';
                ctx.fillText('OPTIMAL', tooltipX + tooltipWidth - 55, tooltipY + 19);
            }

            // Title
            ctx.fillStyle = '#fff';
            ctx.font = 'bold 12px "JetBrains Mono", monospace';
            ctx.fillText(job.name, tooltipX + 12, tooltipY + 24);

            // Stats with icons
            ctx.font = '11px "JetBrains Mono", monospace';

            // Revenue
            ctx.fillStyle = '#2ECC71';
            ctx.fillText('üí∞', tooltipX + 12, tooltipY + 45);
            ctx.fillText(`$${job.revenue.toFixed(0)}`, tooltipX + 32, tooltipY + 45);

            // Risk
            ctx.fillStyle = '#E74C3C';
            ctx.fillText('‚ö†Ô∏è', tooltipX + 100, tooltipY + 45);
            ctx.fillText(`${(job.risk * 100).toFixed(0)}%`, tooltipX + 120, tooltipY + 45);

            // EFE Score
            ctx.fillStyle = '#00D9FF';
            ctx.fillText('üìä', tooltipX + 12, tooltipY + 65);
            ctx.fillText(`EFE: ${job.efe.toFixed(1)}`, tooltipX + 32, tooltipY + 65);

            // Recommendation
            const recommend = job.efe > 150 ? 'Strong Bid' : job.efe > 100 ? 'Consider' : 'Pass';
            const recommendColor = job.efe > 150 ? '#2ECC71' : job.efe > 100 ? '#F1C40F' : '#E67E22';
            ctx.fillStyle = recommendColor;
            ctx.font = 'bold 10px "JetBrains Mono", monospace';
            ctx.fillText(recommend.toUpperCase(), tooltipX + 12, tooltipY + 85);

            // Connection line to job
            ctx.beginPath();
            ctx.moveTo(job.x, job.y);
            ctx.lineTo(tooltipX, tooltipY + tooltipHeight / 2);
            ctx.strokeStyle = 'rgba(255, 255, 255, 0.2)';
            ctx.lineWidth = 1;
            ctx.setLineDash([3, 3]);
            ctx.stroke();
            ctx.setLineDash([]);
        }

        // Axis labels
        ctx.fillStyle = 'rgba(255, 255, 255, 0.5)';
        ctx.font = '11px "JetBrains Mono", monospace';
        ctx.fillText('Revenue ‚Üí', width / 2 - 30, height - 20);

        ctx.save();
        ctx.translate(20, height / 2);
        ctx.rotate(-Math.PI / 2);
        ctx.fillText('‚Üê Lower Risk', -40, 0);
        ctx.restore();
    }
}

// Revenue meter animation with counting
function animateRevenueMeter() {
    const fill = document.getElementById('revenue-fill');
    const current = document.getElementById('revenue-current');
    const milestone = document.getElementById('revenue-milestone');
    
    // Simulate current revenue (would be real data in production)
    const targetPercent = 32; // 32% of daily target
    const targetAmount = 16.00;
    
    // Observe when meter comes into view
    const observer = new IntersectionObserver((entries) => {
        entries.forEach(entry => {
            if (entry.isIntersecting) {
                setTimeout(() => {
                    fill.style.width = targetPercent + '%';
                    animateValue(current, 0, targetAmount, 1500, '$');
                    
                    // Show milestone if past halfway
                    if (targetPercent >= 50) {
                        setTimeout(() => {
                            milestone.style.opacity = '1';
                        }, 1000);
                    }
                }, 300);
                observer.disconnect();
            }
        });
    }, { threshold: 0.3 });
    
    const meter = document.querySelector('.revenue-meter');
    if (meter) observer.observe(meter);
}

// Skill card proficiency animation
function animateSkillCards() {
    const observer = new IntersectionObserver((entries) => {
        entries.forEach(entry => {
            if (entry.isIntersecting) {
                entry.target.classList.add('visible');
            }
        });
    }, { threshold: 0.2 });
    
    document.querySelectorAll('.skill-card').forEach(card => {
        observer.observe(card);
    });
}

// Sticky navigation
function initStickyNav() {
    const nav = document.getElementById('sticky-nav');
    let lastScroll = 0;
    
    window.addEventListener('scroll', () => {
        const currentScroll = window.pageYOffset;
        
        // Show nav after scrolling past header
        if (currentScroll > window.innerHeight * 0.6) {
            nav.classList.add('visible');
        } else {
            nav.classList.remove('visible');
        }
        
        lastScroll = currentScroll;
    });
    
    // Smooth scroll for nav links
    document.querySelectorAll('.nav-link[href^="#"]').forEach(link => {
        link.addEventListener('click', (e) => {
            e.preventDefault();
            const target = document.querySelector(link.getAttribute('href'));
            if (target) {
                target.scrollIntoView({ behavior: 'smooth', block: 'start' });
            }
        });
    });
}

