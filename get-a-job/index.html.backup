            <span class="string">"hours_estimated"</span>: job.estimated_hours,
            <span class="string">"revenue"</span>: outcome.payment,
            <span class="string">"effective_rate"</span>: outcome.payment / outcome.hours,
            <span class="string">"client_rating"</span>: outcome.rating,
            <span class="string">"on_time"</span>: outcome.delivered_at <= job.deadline,
        }
        
        <span class="comment"># Update skill-specific models</span>
        <span class="keyword">for</span> skill <span class="keyword">in</span> job.skills_used:
            skill_model.<span class="function">update</span>(skill, metrics)
        
        <span class="comment"># Bayesian update of market model</span>
        market_model.<span class="function">observe</span>(job.source, outcome.success)
        
        <span class="comment"># Trigger learning</span>
        <span class="keyword">await</span> self.<span class="function">trigger_learning_cycle</span>(metrics)</pre>
